{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": 2,
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": [0, "always-multiline"],
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-return": 0,
    "constructor-super": 2,
    "curly": [2, "all"],
    "default-case": 0,
    "dot-location": [2, "property"],
    "dot-notation": [2, { "allowKeywords": true }],
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "generator-star-spacing": 2,
    "guard-for-in": 2,
    "indent": [0, 4],
    "key-spacing": 2,
    "linebreak-style": 2,
    "max-nested-callbacks": [2, 3],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 0,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-class-assign": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-console": 2,
    "no-continue": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-label": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-label-var": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-process-env": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 0,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-underscore-dangle": 2,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-unused-expressions": 2,
    "no-use-before-define": 0,
    "no-useless-call": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "padded-blocks": [0, "never"],
    "prefer-const": 2,
    "prefer-reflect": 0,
    "prefer-spread": 2,
    "quote-props": [0, "as-needed", {"keywords": true}],
    "quotes": [0, "single"],
    "require-yield": 2,
    "semi": 2,
    "semi-spacing": 2,
    "space-after-keywords": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "strict": 0,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "yoda": 2
  }
}
